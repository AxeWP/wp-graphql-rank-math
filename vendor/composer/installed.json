{
    "packages": [
        {
            "name": "justlevine/wp-graphql-plugin-boilerplate",
            "version": "dev-master",
            "version_normalized": "dev-master",
            "source": {
                "type": "git",
                "url": "https://github.com/justlevine/wp-graphql-plugin-boilerplate.git",
                "reference": "7e65a22fbe5f5c3cd1ed8081ff25b72c01e84e92"
            },
            "dist": {
                "type": "zip",
                "url": "https://api.github.com/repos/justlevine/wp-graphql-plugin-boilerplate/zipball/7e65a22fbe5f5c3cd1ed8081ff25b72c01e84e92",
                "reference": "7e65a22fbe5f5c3cd1ed8081ff25b72c01e84e92",
                "shasum": ""
            },
            "require": {
                "php": ">=7.4 || ^8.0"
            },
            "require-dev": {
                "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
                "phpcompatibility/phpcompatibility-wp": "^2.1",
                "phpstan/extension-installer": "^1.1",
                "phpstan/phpstan": "^1.2",
                "poolshark/wp-graphql-stubs": "^0.0.2",
                "squizlabs/php_codesniffer": "^3.6",
                "szepeviktor/phpstan-wordpress": "^1.0",
                "wp-cli/wp-cli-bundle": "^2.6",
                "wp-coding-standards/wpcs": "^2.3"
            },
            "time": "2022-05-08T10:17:54+00:00",
            "default-branch": true,
            "bin": [
                "bin/create-wpgraphql-plugin.sh"
            ],
            "type": "wordpress-plugin",
            "installation-source": "dist",
            "autoload": {
                "psr-4": {
                    "AxeWP\\GraphQL\\": "src/"
                }
            },
            "scripts": {
                "create-plugin": [
                    "bash bin/create-wpgraphql-plugin.sh"
                ],
                "init-plugin": [
                    "bash bin/init-wpgraphql-plugin.sh"
                ],
                "install-test-env": [
                    "cd wp-graphql-plugin-name && composer install-test-env"
                ],
                "install-stan-env": [
                    "cd wp-graphql-plugin-name && composer --working-dir=wp-graphql-plugin-name/ install-stan-env"
                ],
                "codecept-run": [
                    "cd wp-graphql-plugin-name && vendor/bin/codecept run -c wp-graphql-plugin-name --"
                ],
                "codecept-build": [
                    "cd wp-graphql-plugin-name && /vendor/bin/codecept build -c wp-graphql-plugin-name --"
                ],
                "lint": [
                    "vendor/bin/phpcs"
                ],
                "phpcs-i": [
                    "php ./vendor/bin/phpcs -i"
                ],
                "check-cs": [
                    "php ./vendor/bin/phpcs --standard=.phpcs.xml.dist"
                ],
                "fix-cs": [
                    "php ./vendor/bin/phpcbf --standard=.phpcs.xml.dist"
                ],
                "phpstan": [
                    "phpstan analyze --ansi --memory-limit=1G"
                ],
                "install-project-deps": [
                    "@composer --working-dir=wp-graphql-plugin-name/ install"
                ],
                "update-project-deps": [
                    "@composer --working-dir=wp-graphql-plugin-name/ update"
                ],
                "outdated-project-deps": [
                    "@composer --working-dir=wp-graphql-plugin-name/ outdated"
                ]
            },
            "license": [
                "GPL-3.0-or-later"
            ],
            "authors": [
                {
                    "name": "Dovid Levine",
                    "email": "justlevine@gmail.com"
                }
            ],
            "description": "Boilerplate for creating WPGraphQL extensions",
            "support": {
                "source": "https://github.com/justlevine/wp-graphql-plugin-boilerplate/tree/master",
                "issues": "https://github.com/justlevine/wp-graphql-plugin-boilerplate/issues"
            },
            "install-path": "../justlevine/wp-graphql-plugin-boilerplate"
        }
    ],
    "dev": false,
    "dev-package-names": []
}
