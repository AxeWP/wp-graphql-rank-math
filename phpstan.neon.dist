parameters:
		level: 8
		inferPrivatePropertyTypeFromConstructor: true
		checkMissingIterableValueType: false
		featureToggles:
			disableRuntimeReflectionProvider: true
		stubFiles:
			# Simulate added properties
			- phpstan/rank-math.stub
			- phpstan/class-choices.stub
			- phpstan/class-singular.stub
			- phpstan/class-helpers.stub
			- vendor/justlevine/wp-graphql-plugin-boilerplate/phpstan/class-wp-post-type.stub
			- vendor/justlevine/wp-graphql-plugin-boilerplate/phpstan/class-wp-taxonomy.stub
			- vendor/justlevine/wp-graphql-plugin-boilerplate/phpstan/Model.stub
		bootstrapFiles:
			- phpstan/constants.php
			- wp-graphql-rank-math.php
			- access-functions.php
			- activation.php
			- deactivation.php
			- ../seo-by-rank-math/includes/traits/class-hooker.php
		paths:
			- wp-graphql-rank-math.php
			- access-functions.php
			- activation.php
			- deactivation.php
			- src/
		scanDirectories:
			- src/
			- ../wp-graphql
			- ../seo-by-rank-math
			- vendor/justlevine/wp-graphql-plugin-boilerplate/src/
		ignoreErrors:
			- '#^Function apply_filters(_ref_array)? invoked with ([1-9]|1[0-2]) parameters, 2 required\.$#'
